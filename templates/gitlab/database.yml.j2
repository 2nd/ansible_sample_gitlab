production:
  adapter: postgresql
  encoding: unicode
  database: {{ postgres_db_name }}
  pool: 5
  username: {{ postgres_db_user }}
  password: {{ postgres_db_password }}
  host: localhost
  port: 5432
  socket: /tmp/postgresql.sock

